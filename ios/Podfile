flutter_root = File.expand_path('..', __dir__)
load File.join(flutter_root, '../flutter/bin/flutter_tools/podhelper.rb') if File.exist?(File.join(flutter_root, '../flutter/bin/flutter_tools/podhelper.rb'))

# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'

use_frameworks!
use_modular_headers!

target 'Runner' do
  install_all_flutter_pods(flutter_root)
end
